{
    "name": "Extension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "School Project by me",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "tabs"],
    "background": {
        "scripts": ["urls.js", "background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Extension",
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icons/setting16.png",
        "64": "icons/setting64.png",
        "128": "icons/setting128.png"
    }
}